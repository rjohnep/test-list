{
  "name": "spark-demo",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "homepage": "https://rjohnep.github.io/test-list",
  "author": "@ER",
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "postinstall": "yarn build:dll",
    "prebuild": "yarn build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "build:clean": "rimraf ./dist",
    "start": "cross-env NODE_ENV=development node server",
    "start:production": "yarn build && yarn start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint --cache src/**/*.js",
    "lint:style": "stylelint 'src/**/*.js'",
    "pretest": "yarn test:clean && yarn lint:eslint",
    "test": "jest --coverage",
    "test:clean": "rimraf ./coverage",
    "test:watch": "jest --coverage --watchAll",
    "precommit": "yarn lint:eslint"
  },
  "dllPlugin": {
    "path": "node_modules/spark-demo-dlls",
    "exclude": [
      "compression",
      "cross-env",
      "express",
      "ip"
    ],
    "include": []
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "gh-pages": "^2.0.0",
    "history": "^4.7.2",
    "hoist-non-react-statics": "^3.0.1",
    "immutable": "^3.8.2",
    "invariant": "^2.2.4",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.0",
    "polished": "^2.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-immutable-proptypes": "^2.1.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "styled-components": "^4.0.0-beta.9",
    "typeface-pt-sans": "^0.0.54",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "add-asset-html-webpack-plugin": "^3.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.7.1",
    "circular-dependency-plugin": "^5.0.2",
    "compression": "^1.7.3",
    "cross-env": "^5.1.4",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^1.0.1",
    "image-webpack-loader": "^4.3.1",
    "ip": "^1.1.5",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.2.1",
    "shelljs": "^0.8.1",
    "style-loader": "^0.23.0",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.4.0",
    "svg-sprite-loader": "^4.1.2",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
